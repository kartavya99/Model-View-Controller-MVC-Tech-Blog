<div class="card">
  <div class="card-body">
    <h3 class="card-header">Login</h3>
    <form class="login-form">
      <div class="form-group">
        <label for="exampleInputEmail1" for="username-login">Eamil</label>
        <input
          type="text"
          class="form-control"
          id="email-login"
          autofocus="true"
          class
          aria-describedby="username"
        />
      </div>
      <div class="form-group">
        <label for="password-login">Password</label>
        <input
          type="password"
          class="form-control"
          autofocus="true"
          id="password-login"
        />
      </div>
      <button class="btn btn-primary" type="submit">login</button>

      <hr />
      <a class="login-link" href="/signup">Sign up instead</a>
    </form>

  </div>
</div>
<script src="js/logout.js"></script>
<script src="js/login.js"></script>